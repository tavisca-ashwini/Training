<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>
  <location path="EmployeePage">
    <system.web>
      <authorization>
        <deny roles ="hr"/>
        <allow roles="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="HRPage">
    <system.web>
      <authorization>
        <allow roles="hr"/>
        <deny roles ="*"/>
      </authorization>
    </system.web>
  </location>
</configuration>
